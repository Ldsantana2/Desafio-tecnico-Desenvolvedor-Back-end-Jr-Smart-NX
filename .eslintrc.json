{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "consistent-return": "off",
        "camelcase": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "semi": ["error", "always"]
    }
}
